DirectoryIndex home.html index.html index.php
ErrorDocument 400 https://cadenfinley.com/home.html?error=400
ErrorDocument 401 https://cadenfinley.com/home.html?error=401
ErrorDocument 403 https://cadenfinley.com/home.html?error=403
ErrorDocument 404 https://cadenfinley.com/home.html?error=404
ErrorDocument 500 https://cadenfinley.com/home.html?error=500
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]